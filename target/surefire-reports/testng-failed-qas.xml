<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="QAS Demo">
  <test name="BDD Test">
    <method-selectors>
      <method-selector>
        <script language="beanshell">
          <![CDATA[testngMethod.getMethodName().equalsIgnoreCase("Test Flipkart")]]>
        </script>
      </method-selector>
    </method-selectors>
    <parameter name="env.resources" value="resources"/>
    <parameter name="step.provider.package" value="com.ppiumdemo"/>
    <classes>
      <class name="com.qmetry.qaf.automation.step.client.text.BDDTestFactory"/>
    </classes>
  </test> <!-- BDD Test -->
</suite> <!-- QAS Demo -->
